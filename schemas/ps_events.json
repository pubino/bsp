{
  "contentType": "ps_events",
  "description": "Schema for Events content type",
  "fields": {
    "title": {
      "selector": "[name=\"title[0][value]\"]",
      "type": "text",
      "required": true,
      "label": "Title"
    },
    "body": {
      "selector": "[name=\"field_ps_body[0][value]\"]",
      "type": "textarea",
      "required": false,
      "label": "Event Description"
    },
    "event_start_date": {
      "selector": "[name=\"field_ps_events_date[0][value][date]\"]",
      "type": "date",
      "required": true,
      "label": "Event Start Date"
    },
    "event_start_time": {
      "selector": "[name=\"field_ps_events_date[0][value][time]\"]",
      "type": "time",
      "required": true,
      "label": "Event Start Time"
    },
    "event_end_date": {
      "selector": "[name=\"field_ps_events_date[0][end_value][date]\"]",
      "type": "date",
      "required": false,
      "label": "Event End Date"
    },
    "event_end_time": {
      "selector": "[name=\"field_ps_events_date[0][end_value][time]\"]",
      "type": "time",
      "required": true,
      "label": "Event End Time"
    },
    "location_name": {
      "selector": "[name=\"field_ps_events_location_name[0][value]\"]",
      "type": "text",
      "required": false,
      "label": "Location"
    },
    "status": {
      "selector": "[name=\"status[value]\"]",
      "type": "checkbox",
      "required": false,
      "label": "Published"
    }
  }
}
